[
  {rabbit, [
    {ssl_listeners, [5671]},
    {ssl_options, [{cacertfile,"/etc/rabbitmq/ssl/cacert.pem"},
                    {certfile,"/etc/rabbitmq/ssl/cert.pem"},
                    {keyfile,"/etc/rabbitmq/ssl/key.pem"},
                    {versions, ['tlsv1.2']},
                    {ciphers,  [{rsa,aes_256_cbc,sha256}]},
                    {verify,verify_peer},
                    {fail_if_no_peer_cert,true}]},
    {default_user, <<"sensu">>},
    {default_pass, <<"password">>},
    {default_vhost, <<"/sensu">>},
    {default_permissions, [<<".*">>, <<".*">>, <<".*">>]}
  ]}
].
